<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE queryservice PUBLIC "-//ANYFRAME//DTD QUERYSERVICE//EN"
"http://www.anyframejava.org/dtd/anyframe-core-query-mapping-3.2.dtd">
<queryservice>
	<queries>
		<query id="dynamicReload" isDynamic="false">
			<statement>
				insert into TB_DYNAMIC_RELOAD(col1, col2, col3) values
				(?,?,?)
			</statement>
			<param type="VARCHAR" />
			<param type="VARCHAR" />
			<param type="NUMERIC" />
		</query>		
	</queries>
</queryservice>
